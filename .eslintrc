{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "plugins": ["react", "prettier", "react-hooks"],
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "strict": 0,
        "no-nested-ternary": 0,
        "import/no-named-as-default": 0,
        "prettier/prettier": "warn",
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [".js", ".jsx"]
            }
        ]
    }
}